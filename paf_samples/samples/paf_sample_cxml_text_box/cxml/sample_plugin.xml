<?xml version="1.0" encoding="utf-8"?>
<resource version="0.1" type="normal" id="sample_plugin">

  <pagetable>
    <page id="page_main">
      <plane style="style_plane_sample_black" id="plane_sample_black">
        <layout_hint size="960, 544" />

        <!-- Using a box would be a good idea here -->

        <text style="_common_default_style_text" id="test_strings_id" label="msg_test_text">
          <layout_hint adjust="2, 2" anchor="0, 0" align="0, 0" size="0, 0" pos="0, 150" />
        </text>

        <text_box style="style_text_box_sample" id="sample_text_box" auto_scroll="1" ime_type="0" edit_mode="1" paste_mode="1" edit_auto_transition_mode="0" >
            <layout_hint size="842, 58" pos="0, 80" />
        </text_box>
      </plane>
    </page>
  </pagetable>

  <templatetable>
  </templatetable>

  <styletable>
    <style_plane color=".5, .5, .5, 1" planeobj="plane_obj1" id="style_plane_sample_black">
