<?xml version="1.0" encoding="utf-8"?>
<resource version="0.1" type="normal" id="sample_plugin">

  <pagetable>
    <page id="page_main">
      <plane style="style_plane_sample_black" id="plane_sample_black">
        <layout_hint size="960, 544" />

        <button sound="_common_sound_button_enter" style="_common_default_style_button" label="msg_test_button" id="button">
          <layout_hint size="200, 100" />
        </button>
      </plane>
    </page>
  </pagetable>

  <templatetable>
  </templatetable>

